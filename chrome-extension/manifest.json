{
    "name": "Password Generator",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Generates passwords automatically",
    "browser_action": {
        "default_icon": "lock.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ]
}
